{
  "expo": {
    "name": "Elder connection",
    "slug": "elder-connection-mobile-v2",
    "version": "1.0.0",
    "scheme": "elder-connection-mobile-v2",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-dev-launcher",
        {
          "launchModeExperimental": "most-recent"
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "Cho phép hiện vị trí của bạn trên bản đồ."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Cho phép ứng dụng truy cập vào thư viện ảnh."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/images/appIcon.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      },
      "package": "com.thanhlong109.elderconnectionmobilev2",
      "googleServicesFile": ""
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "286400cc-8053-4959-bfb0-42e92533ab84"
      }
    }
  }
}
